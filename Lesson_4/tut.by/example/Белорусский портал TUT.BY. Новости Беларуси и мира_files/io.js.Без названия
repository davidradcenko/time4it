 _io_setCookie('_io_dev_mod', 1); }; _io_dev_mod_off = function () {
 _io_setCookie('_io_dev_mod', 0, -1); }; _io_is_dev_mod = function () 
{
 return _io_getCookie('_io_dev_mod') == 1; }; _io_qsafe = function (selector, context) {
