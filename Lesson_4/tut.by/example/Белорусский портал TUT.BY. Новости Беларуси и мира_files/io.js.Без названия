 					_io_track_once('quiz_started[' + _io_slices + ']'); }
 				if (answered.indexOf(i) == -1) {
 				 setTimeout(function() {
 						var selected = document.querySelector('#article_body > div > div.Quiz > div:nth-child('+ (i + 1) +')'); var nodeList = selected.querySelectorAll('div[class]')
 						var selected_class = selected.className; var correctness = undefined; var index = undefined; if (selected_class.indexOf('--wrong') > -1) {
 							var el = selected.querySelector('.QuizGroup-item--wrong'); index = Array.prototype.indexOf.call(nodeList, el); }
 						if (selected_class.indexOf('--ok') > -1) {
 							var el = selected.querySelector('.QuizGroup-item--ok'); index = Array.prototype.indexOf.call(nodeList, el); }
 
 						var ans_letter = define_letter(index); if (ans_letter) {
 					 _io_track_once_k('quiz_answer[' + _io_slices + ',marker:' + (i + 1) + ',sub_category:' + ans_letter + ']'); } else {
 					 _io_track_once_k('quiz_answer[' + _io_slices + ',marker:' + (i + 1) + ']'); }
 					answered.push(i); }, 1000); }
 				if (answered.length == questions.length) {
 					_io_track_once('quiz_finished[' + _io_slices + ']'); }
 			}); }(i))
 	}
 	
