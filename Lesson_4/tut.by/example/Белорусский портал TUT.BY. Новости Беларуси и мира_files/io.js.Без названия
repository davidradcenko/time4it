 										}
 
										(function (_title) {
										 if(injected_news[i]){
											var sl = 'smart_block_click[title:' + _title + ',' + ((area)? 'writer:' + encodeURIComponent(area) + ',' : "") + slices + ']'; injected_news[i].addEventListener('click', function() {
