												/*_io_track(sl);*/
											}); }
										}(_title))
										
									/*}*/
								}
							}
						}	
					}
					
					
					if (exclude.indexOf(block_title) == -1) {
						if (blocks[k].news && blocks[k].news.length && blocks[k].news.length > 0) {
							for (var y = 0; y < blocks[k].news.length; y++) {
								track(block[j], blocks[k].news[y]); }
						} else {
							track(block[j], {block: blocks[k].news_block, header_selector: blocks[k].news_title})
						}
					}
					
				}
			}	
		}
	}


