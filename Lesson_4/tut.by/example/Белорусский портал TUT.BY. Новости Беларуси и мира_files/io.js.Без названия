										_title_copy = _title.cloneNode(true); _title_copy.querySelector(".entry-count, .entry-cnt").remove(); _title = _title_copy; } else {
										_title = injected_news[i].querySelector(header_selector); }
									_title = _title.textContent.trim(); _title = encodeURIComponent(_title.replace(/[:,]/g, '_')); var isRegionNews = false; /*if (!isTracked(_title)) {*/
										pushToTrackedNews(_title); if (containerBlock && !containerBlock.getAttribute("data-io-title")) {
											containerBlock.setAttribute("data-io-title", block_title); containerBlock.classList.add("io-smart-block"); }
