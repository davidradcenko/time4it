function isTracked(_title) 
{
	return tracked_news.indexOf(_title) == -1? false : true; }

function getParam(name) 
{
	var getArr = location.search.replace("?",'').split("&"); for(var i = 0; i < getArr.length; i++) {
		var param = getArr[i].split("="); if(param[0] == name.trim()) {
			return param[1]; }
	}; return false; }

function getParamFromLink(name, link) 
{
	var search = link.split("?").pop(); var getArr = search.replace("?",'').split("&"); for(var i = 0; i < getArr.length; i++) {
		var param = getArr[i].split("="); if(param[0] == name.trim()) {
			return param[1]; }
	}; return false; }

function getPageMetrics(sort) {
	if (getParam(sort)) {
		return getParam(sort); } else {
		return getParamFromLink("sort", document.querySelector("#content .c-tabs-i.active a").getAttribute("href")); }
}

function getBlocksListBySort(sort) 
{
	sort = getPageMetrics(sort); switch (sort) {
		case "time" : return [
			{block: '.content .l-columns', is_all_blocks: false, block_title: 'Лента', news_block: '.news-entry', news_title: '.entry-head._title'},
			{block: '.content .l-columns._titlealts_', is_all_blocks: false, block_title_selector: '.news-head h3 a', news_block: '.news-entry', news_title: '.entry-head._title'},
			{block: '.content #latest', is_all_blocks: false, block_title_selector: '.news-head h3', news_block: '.news-entry', news_title: '.entry-head._title'}
		]; break; case "rubric" : return [
			{block: '.content .l-columns', is_all_blocks: false, block_title: 'Картина дня', news: [
				{block: ".b-pod .b-general.ni._titlealts_", header_selector: ".header._title"},
				{block: ".b-pod .b-topc-hot-c", header_selector: ".entry-head._title"},
				{block: ".news-section .news-entry.small", header_selector: ".entry-head._title"},
				{block: ".news-section .news-entry.big", header_selector: ".entry-head._title"}
			]},
			{block: '.content > .l-columns', is_all_blocks: true, block_title_selector: '.news-head h3', news: [
				{block: ".b-pod .b-general.ni._titlealts_", header_selector: ".header._title"},
				{block: ".b-pod .b-topc-hot-c", header_selector: ".entry-head._title"},
				{block: ".news-section .news-entry.small", header_selector: ".entry-head._title"},
				{block: ".news-section .news-entry.big", header_selector: ".entry-head._title"}
			]},
			{block: '.content .b-projects__holder', is_all_blocks: true, block_title_selector: '.head-title', title_replace: /(\.$)|(.BY)/gi, news: [
				{block: ".project-entry", header_selector: ".entry-head._title"},
				{block: ".b-pod .b-topc-hot-c", header_selector: ".entry-head._title"},
				{block: ".news-section .news-entry.small", header_selector: ".entry-head._title"},
				{block: ".news-section .news-entry.big", header_selector: ".entry-head._title"}
			]}
		]; break; default : return [
			{block: '.content .l-columns', is_all_blocks: false, block_title: 'Картина дня', news: [
				{block: ".b-pod .b-general.ni._titlealts_", header_selector: ".header._title"},
				{block: ".b-pod .b-topc-hot-c", header_selector: ".entry-head._title"},
				{block: ".news-section .news-entry.small", header_selector: ".entry-head._title"},
				{block: ".news-section .news-entry.big", header_selector: ".entry-head._title"}
			]},
			{block: '.content > .l-columns', is_all_blocks: true, block_title_selector: '.news-head h3', news: [
				{block: ".b-pod .b-general.ni._titlealts_", header_selector: ".header._title"},
				{block: ".b-pod .b-topc-hot-c", header_selector: ".entry-head._title"},
				{block: ".news-section .news-entry.small", header_selector: ".entry-head._title"},
				{block: ".news-section .news-entry.big", header_selector: ".entry-head._title"}
			]},
			{block: '.content .b-projects__holder', is_all_blocks: true, block_title_selector: '.head-title', title_replace: /(\.$)|(.BY)/gi, news: [
				{block: ".project-entry", header_selector: ".entry-head._title"},
				{block: ".b-pod .b-topc-hot-c", header_selector: ".entry-head._title"},
				{block: ".news-section .news-entry.small", header_selector: ".entry-head._title"},
				{block: ".news-section .news-entry.big", header_selector: ".entry-head._title"}
			]}
		]; break; }
}

var bank = ''; var viewed = []; var writer = ""; function trackNews() {
	
	var blocks = getBlocksListBySort("sort"); var exclude = ["Анекдот дня", "Работа", "Автообъявления"]; var pageMetrics = getPageMetrics("sort"); if (pageMetrics) {
		/*if (_io_slices.indexOf("bank:" + pageMetrics) == -1)
			_io_slices.push("bank:" + pageMetrics);*/
		bank = "bank:" + pageMetrics; }

 function areasPerform(city) 
 {
 switch (city) { 
 case "Брест": return "Брестская область"; break; case "Гродно": return "Гродненская область"; break; case "Гомель": return "Гомельская область"; break; case "Витебск": return "Витебская область"; break; case "Могилев": return "Могилевская область"; break; default: return null; break; }
 
 }
 if(blocks != undefined) for (var k = 0; k < blocks.length; k++) {
		var block; if (blocks[k].is_all_blocks && blocks[k].is_all_blocks == true) {
			block = document.querySelectorAll(blocks[k].block); } else {
			block = document.querySelector(blocks[k].block); }
		if (block) {
			if (!(block.length && block.length > 0)) {
				block = [block]; }
			for (var j = 0; j < block.length; j++) {
				var block_title; var containerBlock = block[j]; if (blocks[k].block_title) {
					block_title = blocks[k].block_title.trim().replace(/[:,]/gi, '_'); } else if (blocks[k].block_title_selector) {
				 if(typeof block[j].querySelector == 'function') {
				 	block_title = block[j].querySelector(blocks[k].block_title_selector); }
				 
					if (block_title) {
						block_title = block_title.textContent.trim().replace(/[:,]/gi, '_'); }
				}

				if (block_title) {

					if (blocks[k].title_replace) {
						block_title = block_title.replace(blocks[k].title_replace, ''); }
					if (block_title.indexOf('Навiны') > -1 && block_title.indexOf('Навiны кампанiй') == -1) {
						block_title = 'Региональная зона'; }
					if (block_title.indexOf('Новости') > -1 && block_title.indexOf('Новости компаний') == -1) {
						block_title = 'Региональная зона'; }
					block_title = translateFromBy(block_title); if (block_title == "Региональная зона" || block_title == encodeURIComponent("Региональная зона")) {
					 var x = document.querySelector("#show_geo_menu"); if (x) {
 area = findArea(x.textContent.trim()); writer = ",writer:" + encodeURIComponent(area); }
					}
					
					block_title = encodeURIComponent(block_title.replace(/[:,]/g, '_')); block_title_slice = (exclude.indexOf(block_title) == -1)? "marker:" + block_title : ""; /*var slices = /*_io_slices.join(',') + block_title_slice;*/
					var slices = bank + ',' + block_title_slice ; if (exclude.indexOf(block_title) == -1) {
						/*_io_track_batch('smart_block_view[' + slices + ']');*/
						var wr = writer; /*_io_track_batch('smart_block_view[' + slices + wr + ']');*/
						if (block_title == encodeURIComponent('Картина дня') || block_title == encodeURIComponent("Лента")) {
