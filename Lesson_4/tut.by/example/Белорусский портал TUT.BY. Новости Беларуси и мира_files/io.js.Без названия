 init_custom : function () {},

 _listen_html5_events : function (html_vid) {
 	var vid = {
 	 slices : {
 title : _io_page(),
 reference_time : html_vid.duration > 3600 ? 7200 : Math.round(html_vid.duration),
 page : _io_page(),
 type : 'html5',
 depth : _io_depth
 	 },
 	 state : {
 	 started : false,
 	 finished : false,
 	 left : false
