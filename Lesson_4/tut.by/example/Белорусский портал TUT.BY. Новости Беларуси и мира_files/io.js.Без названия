 function define_letter(n) {
 	var letter = {
 		1: 'а',
 		2: 'б',
 		3: 'в',
 		4: 'г',
 		5: 'д',
 		6: 'е',
 		7: 'ж',
 		8: 'з',
 		9: 'и',
 		10: 'к'
 	}; if (letter[n])
 		return letter[n]; return n; }
